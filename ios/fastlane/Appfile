apple_id(APPLE_ID) # Your Apple email address

itc_team_id(ITC_TEAM_ID) # App Store Connect Team ID
itc_team_name(ITC_TEAM_NAME)
team_id(APPLE_DEVELOPER_TEAM_ID) # Developer Portal Team ID
team_name(ITC_TEAM_NAME)