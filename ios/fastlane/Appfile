APPLE_ID = ENV["APPLE_ID"]
ITC_TEAM_ID = ENV["ITC_TEAM_ID"]
ITC_TEAM_NAME = ENV["ITC_TEAM_NAME"]
APPLE_DEVELOPER_TEAM_ID = ENV["APPLE_DEVELOPER_TEAM_ID"]

